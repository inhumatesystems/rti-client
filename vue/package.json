    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rollup-plugin-typescript2": "^0.35.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.1",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
