    "dist"
  ],
  "main": "./dist/inhumate-rti-vue.umd.js",
  "module": "./dist/inhumate-rti-vue.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite",
    "dev": "vite",
